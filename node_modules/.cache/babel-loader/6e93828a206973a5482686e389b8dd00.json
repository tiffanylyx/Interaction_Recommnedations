 + \",\" + margin.top + \")\");\n\n\n// Parse the Data\nd3.csv(\"sushi_wordcount.csv\", function(data) {\n/*\ndata.sort(function(b, a) {\n  return a.count - b.count;\n});\n*/\nvar tooltip = d3.select(\"#Wordle\")\n    .append(\"div\")\n    .style(\"width\",'100px')\n    .style(\"position\", \"absolute\")\n    .style(\"z-index\", \"10\")\n    .style(\"visibility\", \"hidden\")\n     .style(\"border\", \"solid\")\n    .style(\"border-width\", \"1px\")\n    .style(\"border-radius\", \"5px\")\n    .style(\"padding\", \"10px\")\n// Add X axis\nvar x = d3.scaleLinear()\n  .domain([0, 130])\n  .range([ 0, width]);\nsvg.append(\"g\")\n  .attr(\"transform\", \"translate(0,\" + height + \")\")\n  .call(d3.axisBottom(x))\n  .selectAll(\"text\")\n    .attr(\"transform\", \"translate(-10,0)rotate(-45)\")\n    .style(\"text-anchor\", \"end\");\n\n// Y axis\nvar y = d3.scaleBand()\n  .range([ 0, height ])\n  .domain(data.map(function(d) { return d.word; }))\n  .padding(1);\nsvg.append(\"g\")\n  .call(d3.axisLeft(y))\n\n// Lines\nsvg.selectAll(\"myline\")\n  .data(data)\n  .enter()\n  .append(\"line\")\n    .attr(\"x1\", function(d) { return x(d.count); })\n    .attr(\"x2\", x(0))\n    .attr(\"y1\", function(d) { return y(d.word); })\n    .attr(\"y2\", function(d) { return y(d.word); })\n    .attr(\"stroke\", \"grey\")\n\n// Circles\nsvg.selectAll(\"mycircle\")\n  .data(data)\n  .enter()\n  .append(\"circle\")\n    .attr(\"cx\", function(d) { return x(d.count); })\n    .attr(\"cy\", function(d) { return y(d.word); })\n    .attr(\"r\", \"7\")\n    .style(\"fill\", \"#6495ED\")\n    .attr(\"stroke\", \"black\")\n    .on(\"mouseover\", function(d){return tooltip.style(\"visibility\", \"visible\").html('Word:'+d.word+'<br/>'+'Count:'+d.count+'  Poet:'+d.name),d3.select(\"#Wordle\").on('mouseover', function(){console.log('hover-wordle-position',d3.mouse(this)); })})\n    .on(\"mousemove\", function(d){return tooltip.style(\"top\",\n    (d3.event.pageY-10)+\"px\").style(\"left\",(d3.event.pageX+10)+\"px\").attr(\"data-html\", \"true\").html('Word:'+d.word+'<br/>'+'Count:'+d.count+'  Poet:'+d.name)})\n    .on(\"mouseout\", function(){return tooltip.style(\"visibility\", \"hidden\")})\n});\n}}\n}\n\n</script>"],"sourceRoot":"src/components"}]}